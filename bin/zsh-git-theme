#!/usr/bin/env bash

# Make sure we're being sourced (possibly by another script). Check for bash
# since zsh sets $0 when sourcing.
if [[ -n "$BASH_VERSION" && "${BASH_SOURCE:-$0}" == "$0" ]]; then
  echo "ERROR: $0 must be sourced."
  exit 1
fi

# git themes from https://github.com/arialdomartini/oh-my-git-themes
#antigen theme arialdomartini/oh-my-git-themes oppa-lana-style
antigen theme arialdomartini/oh-my-git-themes arialdo-granzestyle
#antigen theme arialdomartini/oh-my-git-themes arialdo-pathinline
